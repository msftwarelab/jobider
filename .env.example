# Dice Platform Credentials
DICE_EMAIL=your_dice_email@example.com
DICE_PASSWORD=your_dice_password

# Database Configuration
DATABASE_URL=sqlite:///jobider.db

# Application Settings
LOG_LEVEL=INFO
DICE_SEARCH_QUERY=backend
MAX_APPLICATIONS_PER_DAY=20
APPLICATION_DELAY_MIN=30
APPLICATION_DELAY_MAX=90

# Resume Settings
# Specify the resume filename from the resumes/ folder
# Available: Julian_Thomas.docx, Julian_Thomas.pdf, resume.pdf, etc.
RESUME_FILENAME=your_resume_filename.docx

# Headless Mode (set to false for debugging)
HEADLESS_MODE=true

# Notification Settings (Optional)
ENABLE_NOTIFICATIONS=false
NOTIFICATION_EMAIL=your_notification_email@example.com
